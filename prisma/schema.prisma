generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AthletesRights {
  id                       Int      @id @default(autoincrement())
  name                     String?
  phone                    String?
  is_registered_clt        Boolean?
  had_injury_during_career Boolean?
  injury_description       String?
  injury_timing            String?
  utm_source               String?
  utm_medium               String?
  utm_campaign             String?
  utm_content              String?
  utm_term                 String?
  clicked_whatsapp_button  Boolean?
  webhook_sent             Boolean?
  ip                       String?
  pages_visited            Json?
  created_at               DateTime @default(now())
  updated_at               DateTime @updatedAt

  @@map("athletes_rights")
}

model Retired {
  id                       Int       @id @default(autoincrement())
  name                     String?
  phone                    String?
  score                    String?
  gender                   String?
  birth_date               DateTime?
  contribution_time        String?
  is_unhealthy             Boolean?
  is_military              Boolean?
  utm_source               String?
  utm_medium               String?
  utm_campaign             String?
  utm_content              String?
  utm_term                 String?
  ip                       String?
  device                   String?
  date_benefit_was_granted DateTime?
  webhook_sent             Boolean?
  pages_visited            Json?
  created_at               DateTime  @default(now())
  updated_at               DateTime  @updatedAt

  @@map("retired")
}

model RequestLog {
  id            Int      @id @default(autoincrement())
  method        String
  url           String
  headers       Json?
  body          Json?
  query_params  Json?
  ip            String?
  user_agent    String?
  status_code   Int?
  error_message String?
  error_details Json?
  created_at    DateTime @default(now())

  @@map("request_logs")
}
